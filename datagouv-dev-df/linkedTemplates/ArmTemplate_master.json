{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datagouv-dev-df"
		},
		"LS_ADLStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stgdatagouvdev.dfs.core.windows.net"
		},
		"LS_KV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyValuds-datagouv-dev.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datagouv-dev-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_KV_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KV_properties_typeProperties_baseUrl')]"
					},
					"LS_ADLStorage_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}